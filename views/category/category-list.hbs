<div class="container">
    <h1>Category List</h1>
    {{#each categories}}
        <div class="category-row">
            <a href="/category/{{this._id}}/edit">{{this.catName}}</a>
            <div class="action-links">
                <a href="/category/{{this._id}}/edit" class="btn btn-dark">Edit</a>
                <form action="/category/{{this._id}}/delete"  method="post">
                    <button type ="submit"class="btn btn-warning"> Delete</button>
                </form>
            </div>           
        </div>
    {{/each}}
</div>